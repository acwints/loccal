GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Optional aliases if you prefer Auth.js naming:
# AUTH_GOOGLE_ID=your_google_client_id
# AUTH_GOOGLE_SECRET=your_google_client_secret
NEXTAUTH_SECRET=your_long_random_secret
NEXTAUTH_URL=http://localhost:3000
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/loccal
# Optional: stronger location validation (preferred for production)
# GOOGLE_MAPS_GEOCODING_API_KEY=your_google_maps_geocoding_api_key
# Optional toggles
# LOCCAL_DISABLE_GEOCODING=false
# LOCCAL_ENABLE_NOMINATIM_FALLBACK=true
# Optional: stale threshold for friend snapshots (default 72 hours)
# LOCCAL_SNAPSHOT_STALE_HOURS=72
